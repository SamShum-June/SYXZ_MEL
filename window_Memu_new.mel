                        button  -l "720*1280"    -c "XRSZ_720_1280" -h 18 -w 80 -bgc 1 1 0.6;         //ok
                        button  -l "1080*1920"    -c "XRSZ_1080_1920" -h 18 -w 80 -bgc 1 1 0.6;       //ok
