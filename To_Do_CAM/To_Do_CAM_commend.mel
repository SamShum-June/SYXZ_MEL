                    setAttr "RGB1.mode" 1;
                    setAttr "RGB1.redId" 1;
                    setAttr "RGB1.greenId" 2;
                    setAttr "RGB1.blueId" 3;
                    setAttr "RGB1.enabled" 0;
                    print ("RGB1"+"\n");
                }

